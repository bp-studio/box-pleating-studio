			"magic-string": "^0.27.0"
		},
		"peerDependencyRules": {
			"allowAny": [
				"eslint"
			]
		},
		"patchedDependencies": {
			"ts-node": "patches/ts-node.patch"
		},
		"onlyBuiltDependencies": [
			"@swc/core",
			"core-js",
			"ttf2woff2",
			"unrs-resolver"
		]
	}
}