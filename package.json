		"find-cache-directory": "^6.0.0",
		"gulp": "^5.0.0",
		"gulp-fontawesome": "^1.0.4",
		"gulp-through2": "^1.1.0",
		"html-minifier-loader": "^1.4.1",
		"ifdef-loader": "^2.3.2",
		"inquirer": "^12.6.1",
		"markdown-loader": "^8.0.0",
		"mocha": "^11.4.0",
